{
    "name": "secotrust/sabredav-bundle",
    "type": "symfony-bundle",
    "description": "This bundle integrates the SabreDAV library into Symfony2",
    "keywords": ["SabreDAV", "WebDAV", "CalDAV", "CardDAV", "iCalendar"],
    "homepage": "https://github.com/secotrust/SecotrustSabreDavBundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Henrik Westphal",
            "email": "henrik.westphal@gmail.com"
        },
        {
            "name": "secotrust IT GmbH & Co. KG",
            "homepage": "http://www.secotrust.de/"
        },
        {
            "name": "Community contributions",
            "homepage": "https://github.com/secotrust/SecotrustSabreDavBundle/contributors"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/framework-bundle": "~2.2 || ^3.0",
        "friendsofsymfony/user-bundle": ">=1.3.5",
        "sabre/dav": "~3.1.0"
    },
    "suggest": {
        "knplabs/knp-gaufrette-bundle": "0.2.*",
        "gedmo/doctrine-extensions": "~2.3"
    },
    "autoload": {
        "psr-4": { "Secotrust\\Bundle\\SabreDavBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
